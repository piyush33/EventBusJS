{
  "compilerOptions": {
    "target": "ES6", // or "ES2015" or later
    "module": "ESNext",
    "jsx": "react",
    "lib": ["DOM", "ES6"], // Add "ES6" or "ES2015" to the lib array
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
